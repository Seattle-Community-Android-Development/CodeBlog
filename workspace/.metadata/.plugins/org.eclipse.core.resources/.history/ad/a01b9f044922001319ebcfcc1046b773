<%! private UserAccount activeUser = null; %>

<%!public boolean isLoggedIn() {
	}%>

<%!public void logOut() {
		this.activeUser = null;
	}%>

<%
	this.activeUser = (UserAccount) request.getSession().getAttribute(
			"activeUser");

%>

